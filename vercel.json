{
    "version": 2,
    "builds": [
      {
        "src": "requirements.txt",
        "use": "@vercel/python"
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "todo.wsgi:app"
      }
    ]
  }
  